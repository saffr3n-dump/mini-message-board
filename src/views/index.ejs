<h1><%= title %></h1>

<div class="container">
  <% messages.forEach((msg, i) => { %>
    <div class="message">
      <div>
        <span><%= msg.user %></span>
        <span><%= msg.added %></span>
      </div>
      <p><%= msg.text %></p>
      <a href="/messages/<%= i %>"><button>Open</button></a>
    </div>
  <% }) %>
</div>

<a href="/new"><button>New Message</button></a>
